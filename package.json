{
  "name": "pg-backup-gui",
  "version": "1.0.0",
  "description": "PostgreSQL Database Backup Tool with GUI",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "nodemon --exec electron -r ts-node/register src/main/index.ts",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "package": "npm run build && electron-builder",
    "test": "jest"
  },
  "dependencies": {
    "electron": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^18.0.0",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.0",
    "ts-node": "^10.0.0",
    "@vitejs/plugin-react": "^4.0.0"
  },
  "keywords": [
    "postgresql",
    "backup",
    "gui",
    "electron",
    "database"
  ],
  "author": "pg-backup-gui team",
  "license": "MIT"
}